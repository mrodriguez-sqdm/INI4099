http://www.mulesoft.org/schema/mule/validation http://www.mulesoft.org/schema/mule/validation/current/mule-validation.xsd">
	<sub-flow name="post-validaIdentidadNeglisim-handler" doc:id="c51364db-122e-4544-9466-bda75072e1b0" >
		<ee:transform doc:name="SetVariable Input Body, IdentificationType and Identification" doc:id="3dc573c9-dfb0-44c5-b069-54427be7b454" >
			<ee:message >
			</ee:message>
			<ee:variables >
				<ee:set-variable variableName="identification" ><![CDATA[%dw 2.0
output application/json
---
payload.wsIdentificationValidationNegRequest.identificationValidation.validationObject.identification
]]></ee:set-variable>
				<ee:set-variable variableName="identificationType" ><![CDATA[%dw 2.0
output application/json
---
payload.wsIdentificationValidationNegRequest.identificationValidation.validationObject.identificationType
]]></ee:set-variable>
				<ee:set-variable variableName="inputBody" ><![CDATA[%dw 2.0
output application/json
---
payload]]></ee:set-variable>
				<ee:set-variable variableName="headers" ><![CDATA[%dw 2.0
output application/json
---
attributes.headers]]></ee:set-variable>
			</ee:variables>
		</ee:transform>
		<validation:is-true doc:name="identificationType is number" doc:id="1a5233b1-b845-4a24-bae8-bb707ef85241" expression="#[%dw 2.0 output application/json import isNumeric from dw::core::Strings --- isNumeric(vars.identificationType)]" message='IdentificationType is not a Unicode digit'/>
		<flow-ref doc:name="post-validaIdentidadNeglisim-orchestrator" doc:id="fb1129ba-92e0-4fcc-97ab-af40d1ed40b7" name="post-validaIdentidadNeglisim-orchestrator"/>
	</sub-flow>
