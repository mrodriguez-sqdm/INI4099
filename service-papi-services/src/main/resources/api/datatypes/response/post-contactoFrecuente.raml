          example: "IVR_CONTACTO_FRECUENTE.Servicios.Cuenta_Facturacion"
        cuentaServicio:
          type: string
          required: false
          example: "IVR_CONTACTO_FRECUENTE.Servicios.Cuenta_Servici"
