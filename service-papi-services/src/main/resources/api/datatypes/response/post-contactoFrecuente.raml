          type: string
          required: false
          example: "IVR_CONTACTO_FRECUENTE.Servicios.Estado"
        numeroConexion:
          type: string
          required: false
          example: "IVR_CONTACTO_FRECUENTE.Servicios.Numero_Conexion"
        subEstado:
          type: string
          required: false
          example: "IVR_CONTACTO_FRECUENTE.Servicios.Sub_Estado"
        tecnologia:
          type: string
          required: false
          example: "IVR_CONTACTO_FRECUENTE.Tecnologia"         
  tipoAtencion:
        type: string
        required: false
        example: "IVR_CONTACTO_FRECUENTE.Tipo_Atencion"             