#%RAML 1.0 DataType
type: object
properties:
  status:
    type: string
    enum: [ success, error ]
    required: true
  messageId:
    type: array
    required: true
    description: Lista de identificadores del/los mensaje(s).
    items:
      type: string
      description: UUID del mensaje.
      pattern: '^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-5][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}$'
  details:
    type: string
    required: true