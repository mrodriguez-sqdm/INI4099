#%RAML 1.0 DataType 
type: object
properties:
  codeResponse:
    type: string
    example: '200'
    required: false
  messageResponse:
    type: string
    example: ''
    required: false
  order:
    type: array
    required: false
    items:
      type: object
      properties:
        cun:
          type: string
          example: CUN123456789
          required: false
        numPqr:
          type: string
          example: PQR_123644
          required: false
        orderNumber:
          type: string
          example: ORD987654321
          required: false
        executionDate:
          type: string
          example: '2024-12-25T00:00:00Z'
          required: false
        promotions:
          type: array
          required: false
          items:
            type: object
            properties:
              description:
                type: string
                example: Promoci√≥n de traslado
                required: false
              name:
                type: string
                example: Traslado Gratis
                required: false
