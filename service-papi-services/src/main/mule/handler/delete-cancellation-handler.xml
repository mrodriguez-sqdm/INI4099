<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">
	<sub-flow name="delete-cancellation-handlerSub_Flow" doc:id="00c291fb-df4a-44ab-a77a-e69164b0b009" >
		<set-variable value="#[attributes.uriParams.connectionNumber]" doc:name="set connectionNumber" doc:id="cc8d5f92-66d6-406b-8bc5-de1c4c870ea4" variableName="connectionNumber" />
		<set-variable value="#[attributes.queryParams.product]" doc:name="Set Variable product" doc:id="44a4bdd1-7977-4021-8d9a-dad607a88d8a" variableName="product"/>
		<choice doc:name="Choice" doc:id="d715ab88-6dc4-454d-b164-fb8775d3601e" >
			<when expression='#[upper(attributes.headers.origenTransaccion) == upper("salesforce-xapi-services")]'>
				<set-variable value='#["oracle"]' doc:name="Set logs" doc:id="a2f988ec-865b-4a2e-9208-970084bcf585" variableName="logs" />
				<set-variable value='#["TRANSACCCIONES_OM"]' doc:name="systemId" doc:id="ea2dbc41-3294-43ca-a309-b4579d5ab07b" variableName="systemId" />
			</when>
			<otherwise >
				<set-variable value='#["mongo"]' doc:name="Set logs" doc:id="a3824061-e77b-4d57-a160-a38d476cfdd8" variableName="logs" />
			</otherwise>
		</choice>
		<flow-ref doc:name="Go to orchestrator" doc:id="e20ed245-1fa0-47b6-8f04-a00b1f60896e" name="delete-cancellation-orchestratorSub_Flow"/>
	</sub-flow>
</mule>
