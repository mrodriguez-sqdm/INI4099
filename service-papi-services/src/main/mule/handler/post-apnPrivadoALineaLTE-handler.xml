<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">
	<sub-flow name="post-apnPrivadoALineaLTE-handlerSub_Flow" doc:id="a53abe61-7022-48f9-aa38-9106c0913e6c" >
		<set-variable value="#[payload]" doc:name="inputPayload" doc:id="6e7ae166-e3f1-4e35-a7ef-f5df23f5d060" variableName="inputPayload" />
		<choice doc:name="Choice" doc:id="be961bc0-f058-49ce-aea1-9b0725e0ee2b" >
			<when expression='#[upper(attributes.headers.origenTransaccion) == upper("salesforce-xapi-services")]' >
				<set-variable value='#["oracle"]' doc:name="Set logs" doc:id="85784e3d-e262-4b55-b083-4a2d15470a90" variableName="logs" />
				<set-variable value='#["TRANSACCCIONES_OM"]' doc:name="systemId" doc:id="14bb0eca-a439-4fcb-952a-d479ad072537" variableName="systemId" />
			</when>
			<otherwise >
				<set-variable value='#["mongo"]' doc:name="Set logs" doc:id="a06b483f-6fe2-4b44-a7ce-b722f5f4615a" variableName="logs" />
			</otherwise>
		</choice>
		<flow-ref doc:name="Go to Orchestrator" doc:id="fa340eee-c4b1-43c4-bcfd-b2a918aa97a0" name="post-apnPrivadoALineaLTE-orchestratorSub_Flow" />
	</sub-flow>
</mule>
