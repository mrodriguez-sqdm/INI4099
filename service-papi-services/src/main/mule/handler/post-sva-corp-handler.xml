<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">
	<sub-flow name="post-sva-corp-handlerSub_Flow" doc:id="4dc90655-925c-4e5c-a3a1-e34ee103e862" >
		<set-variable value="#[attributes.headers]" doc:name="headers" doc:id="ad4c5412-1058-4fc2-bb89-04552d78e9f5" variableName="headers" />
		<set-variable value="#[payload]" doc:name="inputPayload" doc:id="a59493bd-c593-4eda-a94e-d172f51457f0" variableName="inputPayload" />
		<choice doc:name="Choice" doc:id="7bd4ac5e-df87-40e5-993f-74a7e7bff29c" >
			<when expression='#[upper(attributes.headers.origenTransaccion) == upper("salesforce-xapi-services")]' >
				<set-variable value='#["oracle"]' doc:name="Set logs" doc:id="d657a47d-6587-4205-a9f0-652f223e3590" variableName="logs" />
				<set-variable value='#["TRANSACCCIONES_OM"]' doc:name="systemId" doc:id="2200b531-a98c-4f87-8222-677e37fb7022" variableName="systemId"/>
			</when>
		</choice>
		<flow-ref doc:name="Go to Orchestrator" doc:id="111bad72-ebe5-4ab9-bdc7-b6479be7776c" name="post-sva-corp-orchestratorSub_Flow" />
	</sub-flow>
</mule>
