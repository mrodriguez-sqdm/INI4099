#%RAML 1.0 DataType 
type: object
properties:
  WSRequestHeader:
    type: !include objects/header.raml
    required: true
  WSRequestBody:
    type: object
    properties:
      Audit:
        type: object
        properties:
          Canal:
            type: string
            example: Web
            required: false
          Id_Device:
            type: string
            example: DEV-12345
            required: false
          SO:
            type: string
            example: Android 12
            required: false
          IP_Address:
            type: string
            example: 192.168.1.100
            required: false
          WhatsApp_Phone_Number:
            type: string
            example: '3001234567'
            required: false
      Contact_Email:
        type: string
        example: cliente@ejemplo.com
        required: false
      Contact_Phone:
        type: string
        example: '3001234567'
        required: false
      Document_Number:
        type: string
        example: '12345678'
        required: false
      Document_Type:
        type: string
        example: CC
        required: false
      Finish_Date:
        type: date-only
        example: '2025-01-19'
        required: false
      Initial_Date:
        type: date-only
        example: '2024-12-19'
        required: false
      Phone:
        type: string
        example: '6013103732'
        required: false

