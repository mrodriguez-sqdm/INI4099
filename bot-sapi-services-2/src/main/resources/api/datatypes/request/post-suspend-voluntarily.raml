#%RAML 1.0 DataType 
type: object
properties:
  WSRequestHeader:
    type: !include objects/header.raml
    required: true
  WSRequestBody:
    type: object
    properties:
      Audit:
        type: object
        properties:
          Canal:
            type: string
            example: Web
            required: false
          Id_Device:
            type: string
            example: ''
            required: false
          SO:
            type: string
            example: ''
            required: false
          IP_Address:
            type: string
            example: ''
            required: false
          IP_Latitude:
            type: string
            example: ''
            required: false
          IP_Longitude:
            type: string
            example: ''
            required: false
          WhatsApp_Phone_Number:
            type: string
            example: ''
            required: false
          Facebook_User:
            type: string
            example: ''
            required: false
          Twitter_User:
            type: string
            example: ''
            required: false
      Phone:
        type: string
        example: '6013103732'
        required: false
      Suspension_Reason:
        type: string
        example: Viaje temporal
        required: false
      Suspension_Period:
        type: integer
        example: 30
        required: false
