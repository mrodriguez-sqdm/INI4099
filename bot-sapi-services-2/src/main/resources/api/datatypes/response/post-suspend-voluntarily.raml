#%RAML 1.0 DataType 
type: object
properties:
  WSResponseHeader:
    type: !include objects/header.raml
    required: true
  Suspension_Result:
    type: object
    properties:
      Phone:
        type: string
        example: '6013103732'
        required: false
      Billing_Account:
        type: string
        example: '15052876387'
        required: false
      Suspension_Status:
        type: string
        example: Procesada
        required: false
      Suspension_Date:
        type: datetime-only
        example: '2024-12-19T10:30:00'
        required: false
      Reactivation_Date:
        type: datetime-only
        example: '2025-01-18T10:30:00'
        required: false
      Confirmation_Number:
        type: string
        example: SUSP-2024-001234
        required: false
      Notes:
        type: string
        example: Suspensi√≥n voluntaria por viaje temporal
        required: false
