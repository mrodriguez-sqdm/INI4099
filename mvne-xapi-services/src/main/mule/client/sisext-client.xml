<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:http="http://www.mulesoft.org/schema/mule/http" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd">
	<sub-flow name="post-captcha" doc:id="9e8e2679-33b9-4f8e-9b86-ca9909b14765" >
		<http:request method="POST" doc:name="Request" doc:id="41729ca5-dee1-49dc-9beb-485135453adb" config-ref="HTTP_Request_configuration_sisext" path="${sisext-sapi.path-captcha}" responseTimeout="#[p('configurationTimeout.sisext-sapi')]" >
			<http:headers ><![CDATA[#[output application/java
---
{
	"source" : p('mietb.source'),
	"name" : vars.initialHeaders.name,
	"X-CORRELATION-ID" : correlationId,
	"processName" : vars.processname,
	"client_secret" : p('sisext-sapi.client_secret'),
	"systemId" : vars.systemId,
	"client_id" : p('sisext-sapi.client_id')
}]]]></http:headers>
			<http:query-params ><![CDATA[#[output application/java
---
{
	secret : vars.secretCaptcha.secret,
	token : vars.captcha
}]]]></http:query-params>
		</http:request>
	</sub-flow>
	<sub-flow name="post-air-recharge" doc:id="5619db15-8ce7-4096-8009-52dcec1d0dff" >
		<http:request method="POST" doc:name="Request" doc:id="9ccfbca0-9b2d-4ab6-befb-a05a9f6ab2fb" config-ref="HTTP_Request_configuration_sisext" path="${sisext-sapi.path-recharge}" responseTimeout="#[p('configurationTimeout.sisext-sapi')]" >
			<http:headers ><![CDATA[#[output application/java
---
{
	"name" : vars.initialHeaders.name,
	"X-CORRELATION-ID" : correlationId,
	"client_secret" : p('sisext-sapi.client_secret'),
	"systemId" : "MIGRACION",
	"client_id" : p('sisext-sapi.client_id')
}]]]></http:headers>
		</http:request>
	</sub-flow>
</mule>
