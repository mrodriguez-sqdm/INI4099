<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:wsc="http://www.mulesoft.org/schema/mule/wsc"
	xmlns:api-gateway="http://www.mulesoft.org/schema/mule/api-gateway"
	xmlns:apikit="http://www.mulesoft.org/schema/mule/mule-apikit" xmlns:http="http://www.mulesoft.org/schema/mule/http"
	xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
http://www.mulesoft.org/schema/mule/mule-apikit http://www.mulesoft.org/schema/mule/mule-apikit/current/mule-apikit.xsd 
http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/api-gateway http://www.mulesoft.org/schema/mule/api-gateway/current/mule-api-gateway.xsd
http://www.mulesoft.org/schema/mule/wsc http://www.mulesoft.org/schema/mule/wsc/current/mule-wsc.xsd">
	<configuration-properties doc:name="Configuration properties" doc:id="bcfeba3e-9f24-4672-a607-5d73fa062f69" file="${env}-properties.yaml" />
		<http:request-config name="HTTP_Request_configuration" doc:name="HTTP Request configuration" doc:id="9c8bda19-393c-42db-8ae9-0458252878b2" basePath="${smallworld.basepath}" responseTimeout="${configurationTimeout.smallworld}">
		<http:request-connection protocol="${smallworld.protocol}" host="${smallworld.host}" port="${smallworld.port}" />
	</http:request-config>
		<http:listener-config
		name="smallworld-sapi-services-httpListenerConfig">
		<http:listener-connection host="${http.host}"
			port="${http.private.port}" />
	</http:listener-config>
	<apikit:config name="smallworld-sapi-services-config"
		api="smallworld-sapi-services.raml"
		outboundHeadersMapName="outboundHeaders"
		httpStatusVarName="httpStatus" />
	<api-gateway:autodiscovery apiId="${autodiscovery}" ignoreBasePath="true" doc:name="API Autodiscovery" doc:id="7fefb804-5234-47b2-be9e-a5a89b85b253" flowRef="smallworld-sapi-services-main" />
	<http:request-config
		name="HTTP_Request_configuration-audits"
		doc:name="HTTP Request configuration"
		doc:id="6b95348c-dd8b-4596-91bf-143a759bd870"
		basePath="${audits-sapi.basePath}">
		<http:request-connection
			host="${audits-sapi.host}"
			port="${audits-sapi.port}" />
	</http:request-config>
	<http:request-config
		name="HTTP_Request_configuration_svcInfraestructura"
		doc:name="HTTP Request configuration"
		doc:id="636f6342-5d2a-4209-8e48-f077cc2d29ca"
		basePath="#[p('svcInfraestructura.basepath')]">
		<http:request-connection
			host="#[p('svcInfraestructura.host')]"
			port="#[p('svcInfraestructura.port')]" />
	</http:request-config>
	<http:request-config
		name="HTTP_Request_configuration_catastro"
		doc:name="HTTP Request configuration"
		doc:id="8eb11823-fe89-4bd6-9318-140db094d5b4"
		basePath="${catastro-bogota.basePath}">
		<http:request-connection
			host="${catastro-bogota.host}"
			port="${catastro-bogota.port}" protocol="${catastro-bogota.protocol}"/>
	</http:request-config>
	<wsc:config name="Web_Service_Consumer_Config_smallworld" doc:name="Web Service Consumer Config" doc:id="24d6fb23-67ac-4882-bf56-5576b835555e" >
		<wsc:connection wsdlLocation="${smallworld-2.soapProtocol}://${smallworld-2.host}:${smallworld-2.port}${smallworld-2.basepath}/${smallworld-2.services.zonaAltoRiesgo.serviceName}?wsdl" service="${smallworld-2.services.zonaAltoRiesgo.serviceName}" port="${smallworld-2.services.zonaAltoRiesgo.portName}" address="${smallworld-2.soapProtocol}://${smallworld-2.host}:${smallworld-2.port}${smallworld-2.basepath}/${smallworld-2.services.zonaAltoRiesgo.serviceName}"/>
	</wsc:config>
</mule>
