---
{
    aplicacion: payload.application,
    ip: payload.ip,
    navegador: payload.browser,
    nombre_equipo: payload.computerName,
    usuario_suma: payload.sumaUser,
    telefono_consultado: payload.queriedPhone,
    archivo_descargado: payload.downloadedFile,
    fecha_hora_registro: payload.registrationDateTime
}
]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<flow-ref doc:name="Flow Reference" doc:id="20143b9b-42e7-401e-ad8f-65457b92d299" name="mongo-db-sapi-create-audit-DIJIN-clientSub_Flow"/>
		<ee:transform doc:name="Transform Message" doc:id="f9180957-4ea4-4fcb-ab72-0da57f6e5198" >
