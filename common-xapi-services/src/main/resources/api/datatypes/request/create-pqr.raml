#%RAML 1.0 DataType 
type: object
properties:
  pqr:
    type: object
    properties:
      customer:
        type: object
        properties:
          documentNumber:
            type: string
            example: '87654321'
            required: false
          documentType:
            type: string
            example: CC
            required: false
          phone:
            type: string
            example: '3057000421'
            required: false
          user:
            type: string
            example: USUARIO_SISTEMA
            required: false
      service:
        type: object
        properties:
          product:
            type: string
            example: INTERNET_FIBRA
            required: false
      detailPqr:
        type: object
        properties:
          contact:
            type: object
            properties:
              documentNumber:
                type: string
                example: '12345678'
                required: false
              documentType:
                type: string
                example: CC
                required: false
              email:
                type: string
                example: cliente@email.com
                required: false
              phone:
                type: string
                example: '3001234567'
                required: false
          cunMassiveFailure:
            type: string
            example: CUN-MASIVA-001
            required: false
          cunRelated:
            type: string
            example: CUN-RELACIONADO-001
            required: false
          description:
            type: string
            example: Descripción detallada del problema
            required: false
          failure:
            type: object
            properties:
              botPeticion:
                type: string
                example: FALLA_INTERNET
                required: false
              causal:
                type: string
                example: FALLA_TECNICA
                required: false
              class:
                type: string
                example: INTERNET
                required: false
              description:
                type: string
                example: No tengo conexión a internet
                required: false
              reason:
                type: string
                example: FALLA TECNICA
                required: false
              symptom:
                type: string
                example: SIN_CONEXION
                required: false
          solution:
            type: string
            example: Solución aplicada
            required: false
          state:
            type: string
            example: ABIERTA
            required: false
          attachment:
            type: array
            items:
              type: object
              properties:
                id:
                  type: string
                  example: ''
                  required: false
                description:
                  type: string
                  example: ''
                  required: false
                attachmentType:
                  type: string
                  example: ''
                  required: false
                mimeType:
                  type: string
                  example: image/png
                  required: false
                file:
                  type: string
                  example: JVBERi0xLjQKJcOkw7zDtsO...
                  required: false
                name:
                  type: string
                  example: March Bill
                  required: false
                size:
                  type: string
                  example: ''
                  required: false
                url:
                  type: string
                  example: ''
                  required: false
