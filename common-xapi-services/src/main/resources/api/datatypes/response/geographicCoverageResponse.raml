#%RAML 1.0 DataType
type: object
displayName: geographicCoverageResponse
description: Response structure for geographic coverage information
properties:
  Result:
    type: object
    description: Result status information
    properties:
      status:
        type: string
        description: Status of the operation
        example: "PASS"
      code:
        type: string
        description: Response code
        example: "OK"
  body:
    type: object
    description: Coverage information body
    properties:
      Feasibility:
        type: object
        description: Feasibility information
        properties:
          Factible:
            type: string
            description: Feasibility status
            example: "NO FACTIBLE"
          Tecnologia:
            type: string
            description: Technology type
            example: "FIBRA"
          Motivo:
            type: string
            description: Reason for feasibility status
            example: "DIRECCION NO ENCONTRADA"
      Tecnologia:
        type: array
        description: Technology coverage information
        items:
          type: object
          properties:
            Tipo:
              type: string
              description: Technology type
              example: "FTTC"
            Propietario:
              type: string
              description: Technology owner
              example: "ETB"
            TipoCobertura:
              type: string
              description: Coverage type
              example: "DISTRITO"
            Cobertura:
              type: string
              description: Coverage area
              example: "DISTRITO"
            NombreCobertura:
              type: string
              description: Coverage name
              example: "31137"
            TipoArea:
              type: string
              description: Area type
              example: "CENTRAL"
            Area:
              type: string
              description: Area name
              example: "YOMASA"
            Estrato:
              type: string
              description: Socioeconomic stratum
              example: "3"
