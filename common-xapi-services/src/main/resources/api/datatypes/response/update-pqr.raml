#%RAML 1.0 DataType 
type: object
properties:
  status:
    type: integer
    example: 200
    required: false
  code:
    type: string
    example: Ok
    required: false
  message:
    type: object
    properties:
      message:
        type: string
        example: Se actualizo la pqr satisfactoriamente
        required: false
  messageServer:
    type: string
    example: ''
    required: false