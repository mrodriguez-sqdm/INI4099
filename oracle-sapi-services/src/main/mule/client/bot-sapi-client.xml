<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:http="http://www.mulesoft.org/schema/mule/http" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd">
	<sub-flow name="get-generate-url-bot-sapi-clientSub_Flow" doc:id="a61facc6-8281-4265-a7dd-a8410d70abe5" >
		<async doc:name="Async" doc:id="2b76ea55-e564-45b2-8872-9e16b75394c9">
			<flow-ref doc:name="Call logger before" doc:id="a4e2fc0b-fdca-4bf0-9672-176f443e5ebf" name="loggin-client-call" />
		</async>
		<http:request method="GET" doc:name="Call to generate Url" doc:id="da24af70-56f1-4d26-a20a-2019ae5cd239" config-ref="HTTP_Request_configuration_bot_sapi" path="${bot-sapi.path}" >
			<http:headers ><![CDATA[#[output application/java
---
{
	"client_secret" : p('bot-sapi.client_secret'),
	"client_id" : p('bot-sapi.client_id'),
	"X-CORRELATION-ID": correlationId,
	"name": attributes.headers.'name',
	"source": attributes.headers.'source',
	"systemId": vars.systemId
}]]]></http:headers>
			<http:uri-params ><![CDATA[#[output application/java
---
{
	email : vars.inputPayload.records[vars.inputPayload.fields indexOf  "EMAIL_ADDRESS_"]
}]]]></http:uri-params>
		</http:request>
		<async doc:name="Async" doc:id="f3f04d88-7cff-457a-80b1-596da6a46b8a">
			<flow-ref doc:name="Call logger after" doc:id="a65e1b7c-15a3-42e5-ac4a-4dbc20a13c64" name="loggin-client-call" />
		</async>
	</sub-flow>
</mule>