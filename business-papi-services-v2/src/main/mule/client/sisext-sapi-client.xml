<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:http="http://www.mulesoft.org/schema/mule/http"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd">
	<sub-flow name="get-employee-sapi-clientSub_Flow" doc:id="f4059b54-e767-4025-bef7-9886c8d70c4a">
					<http:request method="GET" doc:name="Call sisext sapi" doc:id="af7d44cb-650d-4a3d-8eaf-5260258f49a2" config-ref="sisext-Sapi-HTTP_Request_configuration" path="${sisext-sapi.employees.basepath}${sisext-sapi.employees.get.path}">
					<http:headers ><![CDATA[#[output application/java
		---
		{
			"source" : vars.headers.'source',
			"X-CORRELATION-ID": vars.headers.'X-CORRELATION-ID',
			"name" : vars.headers.'name',
			"client_secret" : p('sisext-sapi.client_secret'),
			"client_id" : p('sisext-sapi.client_id'),
			"systemId": vars.headers.systemId,
			"processName": vars.headers.processName
		}]]]></http:headers>
					<http:uri-params ><![CDATA[#[output application/java
		---
		{
			id : vars.numeroIdentificacion
		}]]]></http:uri-params>
				</http:request>
	
	</sub-flow>
</mule>
