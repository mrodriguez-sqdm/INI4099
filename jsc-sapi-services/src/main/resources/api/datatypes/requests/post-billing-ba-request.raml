#%RAML 1.0 DataType
properties:
  name:
    type: string
    required: true
    example: "Creating a new BillingAccount"
  subscriber:
    type: object
    properties:
        id:
          type: string
          required: true
          example: "2"
  cycleConfig:
    type: object
    properties:
        cycleDay:
          type: string
          required: true
          example: "20"