#%RAML 1.0 DataType 
type: object
properties:
  subscription:
    type: object
    properties:
      client:
        type: object
        properties:
          documentNumber:
            type: string
            example: '1234567890'
            required: false
          documentType:
            type: string
            example: CC
            required: false
      acceptsTermsAndConditions:
        type: boolean
        example: true
        required: false
