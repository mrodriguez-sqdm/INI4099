#%RAML 1.0 DataType 
type: object
properties:
  contractNumber:
    type: string
    example: ''
    required: false
  customer:
    type: object
    properties:
      documentNumber:
        type: string
        example: '1234567890'
        required: false
      documentType:
        type: string
        example: CC
        required: false
      name:
        type: string
        example: Juan Carlos Pérez García
        required: false
      phone:
        type: string
        example: '6019980626'
        required: false
  paymentMethod:
    type: object
    properties:
      cvv:
        type: string
        example: '123'
        required: false
      creditCard:
        type: string
        example: '4111111111111111'
        required: false
      creditCardCompany:
        type: string
        example: VISA
        required: false
      expirationDate:
        type: string
        example: 2025/12
        required: false
      token:
        type: string
        example: zdfbdszkjasdfnsjdhgzvzmncxzkmclzaSFNJDSWFAXSCMZXCKNZJVKBZKJ<NVN<JX
        required: false
