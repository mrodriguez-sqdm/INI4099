<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<sub-flow name="post-notifySFOrderBillingFinished-orchestratorSub_Flow" doc:id="50203ebd-20b5-444c-9509-6b8a8ba21f19" >
		<try doc:name="Try" doc:id="257d1132-9cca-4c70-8b6a-33b56e477890" >
			<flow-ref doc:name="Go to client" doc:id="222d001c-67a1-4484-8feb-ccf773ac1b8c" name="post-notifySFOrderBillingFinished-clientSub_Flow" />
			<ee:transform doc:name="Transform to JSON" doc:id="dfad28c4-b8ff-44be-98ee-aa3adc843594">
			<ee:message>
				<ee:set-payload><![CDATA[%dw 2.0
output application/json
---
{
  "Resultado": {
    "code": "SUCCESS",
    "description": "SUCCESS"
  }, 
  "Document": {
    "id": payload.'_id'[0]
  }
}
]]></ee:set-payload>
			</ee:message>
			<ee:variables>
				<ee:set-variable variableName="httpStatus"><![CDATA[201]]></ee:set-variable>
			</ee:variables>
		</ee:transform>
			<error-handler >
				<on-error-continue enableNotifications="true" logException="true" doc:name="On Error Continue" doc:id="0791b7ff-c4c6-4e85-a6e4-ce42ed1634be" >
					<ee:transform doc:name="Transform to JSON" doc:id="919325df-646a-432e-9997-5e710276460e" >
						<ee:message >
							<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
{
  "Resultado": {
    "messageCode": "ERROR",
    "message": error.description
  }, 
  "Document": {
    "id": ""
  }
}
]]></ee:set-payload>
						</ee:message>
						<ee:variables >
							<ee:set-variable variableName="httpStatus" ><![CDATA[500]]></ee:set-variable>
						</ee:variables>
					</ee:transform>
				</on-error-continue>
			</error-handler>
		</try>
	</sub-flow>
</mule>
