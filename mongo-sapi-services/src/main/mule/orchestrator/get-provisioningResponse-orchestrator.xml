<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<sub-flow name="get-provisioningResponse-orchestratorSub_Flow" doc:id="bd41201c-e4c4-4aa9-ab0b-5c365c2c04ef" >
		<flow-ref doc:name="Go to client" doc:id="6e9c2e72-779e-40b1-b531-28db38b15d0d" name="get-provisioningResponse-clientSub_Flow"/>
		<ee:transform doc:name="Output" doc:id="de0bc84c-ea90-448c-8602-30601e73d468">
			<ee:message>
				<ee:set-payload><![CDATA[%dw 2.0
var resultados = if(payload != []) payload map ($) - "_id" else null
var resultadoMsg= if(payload != []) "Positivo" else "Negativo"
var descMsg = if(payload != []) null else "No se encontro una order con el ordenOM: " ++ vars.ordenOm
output application/json skipNullOn="everywhere"
---
{
    resultados: resultados,
    resultado: resultadoMsg,
    fecha: now(),
    descripcion: descMsg
}

]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<logger level="INFO" doc:name="Logger output" doc:id="2f83d172-a960-4ac3-b08f-af0b2247d44d" message="#[payload]" />
	</sub-flow>
</mule>
