<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<sub-flow name="post-interaction-orchestratorSub_Flow" doc:id="e73e4416-4f4f-44b5-8ee0-107f5fe80d4d" >
		<flow-ref doc:name="Go to client" doc:id="a08bd698-13e3-4d6b-b398-1fb27847e73d" name="post-interaction-clientSub_Flow"/>
		<ee:transform doc:name="Map Out" doc:id="44d55dd2-3999-47f3-b1e6-37a635c69080">
			<ee:message>
				<ee:set-payload><![CDATA[%dw 2.0
output application/json	
---
if(payload."_id" != null)
{
	"codigo": "OK",
	"descripcion": "Registro insertado"
}
else 
{
	"codigo": "ERROR",
	"descripcion": "Registro no se pudo insertar"
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</sub-flow>
</mule>
