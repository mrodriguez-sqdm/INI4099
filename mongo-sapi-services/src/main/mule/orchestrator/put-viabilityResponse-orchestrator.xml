<?xml version="1.0" encoding="UTF-8"?>

<mule xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core" xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<sub-flow name="put-viabilityResponse-orchestratorSub_Flow" doc:id="49000072-e8c5-4bc4-be35-ff21d0348c19" >
		<flow-ref doc:name="Go to Client" doc:id="8964b3ee-d0ca-44f8-a14e-d3fae4ee77eb" name="put-viabilityResponse-clientSub_Flow"/>
		<ee:transform doc:name="Output" doc:id="6ea45c5d-7055-4cff-a169-39dcb3d0bb75" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
var resultadoMsg = if(payload.modified > 0) 'Positivo' else "Negativo"
var numAffectedRows = payload.modified
var descripcionMsg = if(payload.modified > 0) null else "No se actualizÃ³ la orden"
output application/json skipNullOn="everywhere"
---
{
	resultado: resultadoMsg,
    registrosAfectados: numAffectedRows,
    fecha: now(),
    descripcion: descripcionMsg
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<logger level="INFO" doc:name="Log Output" doc:id="02b5de6c-e61a-4426-a1ad-d4ea890e8a2a" message="#[payload]" />
	</sub-flow>
</mule>
