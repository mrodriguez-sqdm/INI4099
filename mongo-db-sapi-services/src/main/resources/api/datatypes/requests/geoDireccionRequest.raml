#%RAML 1.0 DataType
type: object
displayName: geoDireccionRequest
description: Request structure for updating geographic address information
properties:
  geoAddress:
    type: object
    properties:
      chip:
        type: string
        required: true
        minLength: 1
        description: CHIP identifier
        example: "CHIP123456789"
      propertyClass:
        type: string
        required: true
        minLength: 1
        description: Property class
        example: "RESIDENCIAL"
      detailedAddress:
        type: string
        required: true
        minLength: 1
        description: Complete address with complements (apartment, floor, office, etc.)
        example: "CL 16D 65A 56 SUR PI 2 APT 30"
      catastroStratum:
        type: string
        required: true
        description: Catastro stratum
        example: "6"
      detailUpdateDate:
        type: string
        required: true
        minLength: 1
        description: Detail update date
        example: "2024-01-15T10:30:00Z"
      addressModificationDate:
        type: string
        required: true
        minLength: 1
        description: Address modification date
        example: "2024-01-15T10:30:00Z"
      addressId:
        type: string
        required: true
        minLength: 1
        description: Address identifier
        example: "ADDR123456789"
      neighborhoodId:
        type: string
        required: true
        minLength: 1
        description: Neighborhood identifier
        example: "BARRIO001"
      departmentId:
        type: string
        required: true
        description: Department identifier
        example: "11"
      localityId:
        type: string
        required: true
        description: Locality identifier
        example: "1"
      municipalityId:
        type: string
        required: true
        description: Municipality identifier
        example: "11001"
      propertyId:
        type: string
        required: true
        minLength: 1
        description: Property identifier
        example: "PREDIO123456"
      latitude:
        type: number
        required: true
        minimum: -90
        maximum: 90
        description: Latitude coordinate (must be between -90 and 90)
        example: 4.53091498900005
      longitude:
        type: number
        required: true
        minimum: -180
        maximum: 180
        description: Longitude coordinate (must be between -180 and 180)
        example: -74.117704933
      property:
        type: string
        required: true
        minLength: 1
        description: Property name
        example: "Torre Norte"
      actionType:
        type: string
        required: true
        minLength: 1
        description: Action type
        example: "UPDATE"
      distanceType:
        type: string
        required: true
        minLength: 1
        description: Distance type
        example: "EXACTA"
      addressType:
        type: string
        required: true
        minLength: 1
        description: Address type
        example: "RESIDENCIAL"
      updateExtractor:
        type: string
        required: false
        description: Update extractor information
        example: "EXTRACTOR001"
      currentAddress:
        type: string
        required: true
        minLength: 1
        description: Base address without complements (building/property location)
        example: "CL 16D 65A 56 SUR"
      municipality:
        type: string
        required: false
        description: Municipality name
        example: "Bogot√°"
      locality:
        type: string
        required: false
        description: Locality name
        example: "Chapinero"
      neighborhood:
        type: string
        required: false
        description: Neighborhood name
        example: "Centro"
