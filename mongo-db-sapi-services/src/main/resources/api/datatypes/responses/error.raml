#%RAML 1.0 DataType 
type: object
properties:
  status:
    type: integer
    example: 500
    required: false
  code:
    type: string
    example: INTERNAL SERVER ERROR
    required: false
  message:
    type: string
    example: Required query parameter document not specified
    required: false
  messageServer:
    type: string
    example: ''
    required: false