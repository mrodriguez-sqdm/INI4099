#%RAML 1.0 DataType 
type: object
properties:
  code:
    type: string
    example: '200'
    required: false
  message:
    type: string
    example: operacion exitosa
    required: false
  agendas:
    type: array
    items:
      type: object
      properties:
        _id:
          type: object
          properties:
            $oid:
              type: string
              example: DcA8dbB7bEBcaBbadcEafBe2
              required: false
        action_type:
          type: string
          example: INSTALACION
          required: false
        address:
          type: string
          example: 'Calle 45 # 18-22 Bogot√°'
          required: false
        agendas_tmr:
          type: array
          items:
            type: object
            properties:
              _id:
                type: object
                properties:
                  $oid:
                    type: string
                    example: 5ffcbb2FeD132BEc1dfc304a
                    required: false
              estado_agenda:
                type: string
                example: PROGRAMADA
                required: false
              fecha_creacion:
                type: object
                properties:
                  $date:
                    type: datetime
                    example: '2025-02-12T09:15:45.120Z'
                    required: false
              fecha_inicio_agenda:
                type: array
                required: false
                items:
                  type: datetime
              id_eta:
                type: string 
                example: ETA-123456789
                required: false
              solicitado_cliente:
                type: boolean
                example: true
                required: false
              timeslot:
                type: string
                example: AM
                required: false
              canal:
                type: string
                example: CALL_CENTER
                required: false
              fecha_fin_agenda:
                type: datetime
                example: '2025-02-15T10:30:00.000Z'
                required: false
              id_osm:
                type: string
                example: OSM-99887766
                required: false
              motivo_reagendamiento:
                type: string
                example: CLIENTE NO DISPONIBLE
                required: false
              oportunidad_agenda:
                type: number
                example: 0.35
                required: false
              usuario_creador:
                type: string
                example: jramirez
                required: false
              usuario_modificador:
                type: string
                example: jramirez
                required: false
        connection_number:
          type: string
          example: '3014567890'
          required: false
        cun:
          type: string
          example: '5425454'
          required: false
        customer_document_number:
          type: string
          example: '1020304050'
          required: false
        customer_document_type:
          type: string
          example: CC
          required: false
        duration:
          type: string
          example: '02:15:00'
          required: false
        fecha_actualizacion:
          type: object
          properties:
            $date:
              type: datetime
              example: '2025-02-20T17:40:12.300Z'
              required: false
        order_number:
          type: string
          example: SO-1234567
          required: false
        order_type:
          type: string
          example: INSTALACION
          required: false
        origin_system:
          type: string
          example: SIEBEL
          required: false
        service_type:
          type: string
          example: INTERNET
          required: false
        skill:
          type: string
          example: FTTH
          required: false
        technician_document_number:
          type: string
          example: '900123456'
          required: false
        technician_document_type:
          type: string
          example: CC
          required: false
        technology:
          type: string
          example: FIBRA
          required: false
        usuario_creo:
          type: string
          example: jramirez
          required: false
        zone:
          type: string
          example: ZONA-7-BOSA
          required: false
