#%RAML 1.0 DataType
type: object
properties:
  status:
    type: number
    required: true
    description: Indicates the HTTP error status code returned
    example: 404
  message:
    type: string
    required: true
    description: Brief description of what caused the error
    example: "There are no matching records"
  id:
    type: string
    required: true
    description: number or serviceAccount ID
    example: "MDM-MAY-0001"