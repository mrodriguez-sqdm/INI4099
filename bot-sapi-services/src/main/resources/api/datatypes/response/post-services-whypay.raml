#%RAML 1.0 DataType 
type: object
properties:
  WSResponseHeader:
    type: !include objects/header.raml
    required: true
  WSResponseBody:
    type: object
    properties:
      Phone:
        type: string
        example: '6013103732'
        required: false
      Billing_Account:
        type: string
        example: '15052876387'
        required: false
      Billing_Number:
        type: string
        example: FAC2024001234
        required: false
      Billing_Number_PSE:
        type: string
        example: PSE2024001234
        required: false
      Billing_Address:
        type: string
        example: 'Calle 123 #45-67, Bogotá'
        required: false
      Billing_Phone:
        type: string
        example: '6013103732'
        required: false
      Email:
        type: string
        example: cliente@email.com
        required: false
      Billed_Period:
        type: string
        example: 2024-12
        required: false
      Billing_Cutoff_Date:
        type: date-only
        example: '2024-12-15'
        required: false
      Payment_Date:
        type: date-only
        example: '2024-12-25'
        required: false
      Payday_Limit:
        type: date-only
        example: '2024-12-30'
        required: false
      State:
        type: string
        example: Vencida
        required: false
      Delivery_Type:
        type: string
        example: Digital
        required: false
      Extemporaneous:
        type: boolean
        example: false
        required: false
      Is_Receipt_Of_Payment:
        type: boolean
        example: false
        required: false
      Invoice_Origin:
        type: string
        example: Sistema de Facturación
        required: false
      Barcode:
        type: string
        example: '12345678901234567890'
        required: false
      Value_Original:
        type: object
        properties:
          Value:
            type: integer
            example: 125000
            required: false
          Value_Str:
            type: string
            example: $125,000.00
            required: false
      Value_Service:
        type: object
        properties:
          Value:
            type: integer
            example: 89000
            required: false
          Value_Str:
            type: string
            example: $89,000.00
            required: false
      Value_Final:
        type: object
        properties:
          Value:
            type: integer
            example: 125000
            required: false
          Value_Str:
            type: string
            example: $125,000.00
            required: false
      Concepts:
        type: object
        properties:
          Reconnected:
            type: object
            properties:
              Cost:
                type: integer
                example: 15000
                required: false
              Cost_Usr:
                type: string
                example: $15,000.00
                required: false
              Date:
                type: date-only
                example: '2024-12-10'
                required: false
          Suspend:
            type: object
            properties:
              Cost:
                type: integer
                example: 10000
                required: false
              Cost_Usr:
                type: string
                example: $10,000.00
                required: false
              Date:
                type: date-only
                example: '2024-12-05'
                required: false
      Information_Value:
        type: array
        items:
          type: object
          properties:
            Date:
              type: date-only
              example: '2024-12-20'
              required: false
            Message:
              type: string
              example: El valor a pagar incluye servicios adicionales del mes anterior
              required: false
      Payment:
        type: object
        properties:
          Bank:
            type: string
            example: Bancolombia
            required: false
          Payment_Date:
            type: date-only
            example: '2024-12-28'
            required: false
          Type:
            type: string
            example: Digital
            required: false
          Value:
            type: object
            properties:
              Value:
                type: integer
                example: 125000
                required: false
              Value_Str:
                type: string
                example: $125,000.00
                required: false
      Adjust:
        type: object
        properties:
          Date:
            type: date-only
            example: '2024-12-15'
            required: false
          Num_PQR:
            type: string
            example: PQR2024001234
            required: false
          Origin:
            type: string
            example: Reclamo Cliente
            required: false
          Value:
            type: object
            properties:
              Value:
                type: integer
                example: -5000
                required: false
              Value_Str:
                type: string
                example: '-$5,000.00'
                required: false
