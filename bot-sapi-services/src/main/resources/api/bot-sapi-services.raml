#%RAML 1.0
title: bot-sapi-services
baseUri: bot-sapi-services-dev.us-e2.cloudhub.io/bot
description: The following API will be in charge of the comunications towards bot services.
mediaType: 
 - application/json
version: "1"
protocols:
  - HTTP

traits:
  security_client_id: !include exchange_modules/35a39a92-efa9-49b6-8f13-d66508fd2f35/security-clientid/1.0.2/security-clientid.raml
  request_headers: !include exchange_modules/35a39a92-efa9-49b6-8f13-d66508fd2f35/tibcorequestheaders/1.0.5/tibcorequestheaders.raml
  encrypted_headers: !include datatypes/request/objects/encrypt.raml

/operations:
  /v1:
    /generateRegisterUrl:
      /{email}:
        uriParameters:
          email:
            type: string
            required: true
            example: "nicolascastiblancob@gmail.com"
        get:
          is: [security_client_id, request_headers]
          displayName: Generate Register URL
          description: Generates a URL for resgistration
          responses:
            201:
              body:
                application/json:
                  type:
                    !include datatypes/response/generateRegisterURL.raml
/saleOrder:
  /encripted:
    /v1:
      post:
        is: [security_client_id, request_headers, encrypted_headers]
        displayName: Get Encripted Data
        description: Get a encripted data from bot services
        body:
          application/json:
            type: 
              !include datatypes/request/postEncriptedData.raml
            example:
              !include examples/request/postEncriptedData.json
        responses:
          200:
            body:
              application/json:
                type: 
                  !include datatypes/response/postEncriptedData.raml
                example:
                  !include examples/response/postEncriptedData.json
            headers:
              x_civ_response:
                description: "Par√°metro que debe ser incluido en la cabecera de la respuesta que cumple el objetivo de des-cifrar el contenido (IV)"
                type: string
                required: false
